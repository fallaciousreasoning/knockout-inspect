{
  "name": "knockoutinspect",
  "version": "1.0.0",
  "description": "An inspector for Knockout",
  "targets": {
    "webext-dev": {}
  },
  "scripts": {
    "start": "run-pty % npm run typescript % npm run build",
    "typescript": "tsc --watch",
    "build": "web-ext run --source-dir . --firefox=nightly --start-url=https://knockoutjs.com/",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/firefox-webext-browser": "^82.0.1",
    "@types/knockout": "^3.4.71",
    "typescript": "^4.3.5"
  },
  "devDependencies": {
    "run-pty": "^2.3.2",
    "web-ext": "^6.3.0"
  }
}
